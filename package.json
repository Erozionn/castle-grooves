{
  "name": "crastle-grooves",
  "version": "0.2.0",
  "description": "Castle Grooves music bot.",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "start:dev": "yarn build && nodemon -r dotenv/config build/index.js dotenv_config_path=.env.dev dotenv_config_debug=true",
    "build": "rimraf ./build && tsc",
    "deploy-commands": "node -r dotenv/config deploy-commands.js dotenv_config_path=.env.dev dotenv_config_debug=true",
    "postinstall": "patch-package",
    "format": "npx prettier *.{js,ts,json} --write",
    "lint": "eslint --ext .ts,.js,.cts,.cjs .",
    "lint:fix": "eslint --fix --ext .ts,.js,.cts,.cjs .",
    "p": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Erozionn/castle-grooves.git"
  },
  "author": "Alexander Tasciyan",
  "license": "MIT",
  "homepage": "https://github.com/Erozionn/castle-grooves#readme",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^0.5.0",
    "@discordjs/voice": "^0.8.0",
    "@distube/yt-dlp": "^1.1.3",
    "@distube/ytdl-core": "^4.11.9",
    "@influxdata/influxdb-client": "^1.26.0",
    "discord-api-types": "^0.34.0",
    "discord.js": "^14.8.0",
    "distube": "^4.0.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fast-average-color-node": "^2.2.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^4.4.1",
    "libsodium-wrappers": "^0.7.9",
    "patch-package": "^6.4.7",
    "skia-canvas": "^1.0.1",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0-0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "imports": {
    "#utils/*": "./utils/*",
    "#api": "./api/index.js",
    "#commands/*": "./commands/*",
    "#constants/*": "./constants/*"
  }
}
