{
  "name": "castle-grooves",
  "version": "0.2.0",
  "description": "Castle Grooves music bot.",
  "main": "index.ts",
  "scripts": {
    "start": "node -r dotenv/config build/index.js dotenv_config_path=.env.dev dotenv_config_debug=true",
    "start:dev": "ts-node-dev --inspect -r tsconfig-paths/register -r dotenv/config index.ts dotenv_config_path=.env.dev dotenv_config_debug=true",
    "start:prod": "node build/index.js",
    "start:prefill": "NOW_PLAYING_MOCK_DATA=true yarn start:dev",
    "start:docker": "docker run --env-file .env.dev --name castle-grooves castle-grooves:dev",
    "build": "rimraf ./build && tsc && tsc-alias",
    "build:docker": "yarn build && docker build -t castle-grooves:dev .",
    "deploy-commands": "ts-node -r tsconfig-paths/register -r dotenv/config deploy-commands.ts dotenv_config_path=.env.dev dotenv_config_debug=true",
    "postinstall": "patch-package",
    "nuke": "rimraf ./node_modules && yarn",
    "format": "npx prettier *.{js,ts,json} --write",
    "lint": "eslint --ext .ts,.js,.cts,.cjs .",
    "lint:fix": "eslint --fix --ext .ts,.js,.cts,.cjs .",
    "p": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Erozionn/castle-grooves.git"
  },
  "author": "Alexander Tasciyan",
  "license": "MIT",
  "homepage": "https://github.com/Erozionn/castle-grooves#readme",
  "dependencies": {
    "@discord-player/extractor": "^7.1.0",
    "@discordjs/rest": "^2.4.3",
    "@discordjs/voice": "^0.16.1",
    "@influxdata/influxdb-client": "^1.26.0",
    "@napi-rs/canvas": "^0.1.70",
    "bgutils-js": "^3.2.0",
    "bufferutil": "^4.0.9",
    "date-fns": "^3.6.0",
    "discord-api-types": "^0.37.119",
    "discord-player": "^7.1.0",
    "discord-player-deezer": "^2.5.0",
    "discord-player-soundcloud": "^1.0.7",
    "discord-player-spotify": "^1.1.7",
    "discord-player-youtubei": "^1.5.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.2.0",
    "googlevideo": "^4.0.4",
    "libsodium-wrappers": "^0.7.13",
    "mediaplex": "^1.0.0",
    "module-alias": "^2.2.3",
    "patch-package": "^8.0.0",
    "sharp": "^0.33.2",
    "utf-8-validate": "^6.0.3",
    "youtubei.js": "^16.0.1",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@types/babel__core": "^7",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0-0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "_moduleAliases": {
    "@": ".",
    "@utils": "utils",
    "@api": "api",
    "@commands": "commands",
    "@components": "components",
    "@constants": "constants",
    "@types": "types",
    "@hooks": "hooks",
    "@data": "data"
  },
  "packageManager": "yarn@4.9.1"
}
