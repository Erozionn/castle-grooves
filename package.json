{
  "name": "crastle-grooves",
  "version": "0.2.0",
  "description": "Castle Grooves music bot.",
  "main": "index.ts",
  "scripts": {
    "start": "node -r dotenv/config build/index.js dotenv_config_path=.env.dev dotenv_config_debug=true",
    "start:dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config index.ts --inspect dotenv_config_path=.env.dev dotenv_config_debug=true",
    "start:prod": "node build/index.js",
    "start:prefill": "NOW_PLAYING_MOCK_DATA=true yarn start:dev",
    "start:docker": "docker run --env-file .env.dev --name castle-grooves castle-grooves:dev",
    "build": "rimraf ./build && tsc && tsc-alias",
    "build:docker": "yarn build && docker build -t castle-grooves:dev .",
    "deploy-commands": "node -r dotenv/config deploy-commands.js dotenv_config_path=.env.dev dotenv_config_debug=true",
    "postinstall": "patch-package",
    "nuke": "rimraf ./node_modules && yarn",
    "format": "npx prettier *.{js,ts,json} --write",
    "lint": "eslint --ext .ts,.js,.cts,.cjs .",
    "lint:fix": "eslint --fix --ext .ts,.js,.cts,.cjs .",
    "p": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Erozionn/castle-grooves.git"
  },
  "author": "Alexander Tasciyan",
  "license": "MIT",
  "homepage": "https://github.com/Erozionn/castle-grooves#readme",
  "dependencies": {
    "@discord-player/extractor": "^4.4.6",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.16.1",
    "@influxdata/influxdb-client": "^1.26.0",
    "discord-api-types": "^0.37.70",
    "discord-player": "^6.6.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.1",
    "libsodium-wrappers": "^0.7.13",
    "module-alias": "^2.2.3",
    "patch-package": "^8.0.0",
    "sharp": "^0.33.2",
    "skia-canvas": "^1.0.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0-0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@": ".",
    "@utils": "utils",
    "@api": "api",
    "@commands": "commands",
    "@components": "components",
    "@constants": "constants",
    "@types": "types",
    "@hooks": "hooks",
    "@data": "data"
  },
  "packageManager": "yarn@4.1.0"
}
